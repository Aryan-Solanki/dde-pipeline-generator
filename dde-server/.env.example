# ==========================================
# DDE Backend Server - Environment Configuration
# ==========================================
# Copy this file to .env and add your UPB AI Gateway credentials

# REQUIRED: UPB AI Gateway API Key
# Get your API key from: https://ai-gateway.uni-paderborn.de/
UPB_API_KEY=your_api_key_here

# REQUIRED: UPB AI Gateway Base URL
UPB_BASE_URL=https://ai-gateway.uni-paderborn.de/v1/

# OPTIONAL: AI Model (default: gwdg.qwen3-30b-a3b-instruct-2507x)
# UPB_MODEL=gwdg.llama-3.3-70b-instruct

# OPTIONAL: Rate Limit (requests per minute, default: 10)
# RPM_LIMIT=10

# OPTIONAL: Service URLs (defaults shown)
# PORT=5050
# VALIDATOR_URL=http://localhost:5051

# OPTIONAL: Environment
# NODE_ENV=development
